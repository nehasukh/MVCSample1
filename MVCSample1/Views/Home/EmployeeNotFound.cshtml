@model int
@{ 
    ViewBag.Title = "404 Error";
}

<div class="alert alert-danger mt-1 mb-1">
    <h4> 404 Not Found Error : </h4>
    <hr />
    <h5>
        Employee with id = @Model can not be found
    </h5>
</div>
<a asp-controller="Home" asp-action="GetAllemployee" class="btn btn-outline-success">
    Click here to see the list of all employees
</a>